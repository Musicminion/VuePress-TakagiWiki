#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/takagisite/node_modules/.pnpm/valaxy@0.14.30_@types+markdown-it@12.2.3_@types+node@20.2.5_@unhead+vue@1.1.27_postcss@8.4.24_typescript@4.8.4/node_modules/valaxy/bin/node_modules:/workspaces/takagisite/node_modules/.pnpm/valaxy@0.14.30_@types+markdown-it@12.2.3_@types+node@20.2.5_@unhead+vue@1.1.27_postcss@8.4.24_typescript@4.8.4/node_modules/valaxy/node_modules:/workspaces/takagisite/node_modules/.pnpm/valaxy@0.14.30_@types+markdown-it@12.2.3_@types+node@20.2.5_@unhead+vue@1.1.27_postcss@8.4.24_typescript@4.8.4/node_modules:/workspaces/takagisite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/takagisite/node_modules/.pnpm/valaxy@0.14.30_@types+markdown-it@12.2.3_@types+node@20.2.5_@unhead+vue@1.1.27_postcss@8.4.24_typescript@4.8.4/node_modules/valaxy/bin/node_modules:/workspaces/takagisite/node_modules/.pnpm/valaxy@0.14.30_@types+markdown-it@12.2.3_@types+node@20.2.5_@unhead+vue@1.1.27_postcss@8.4.24_typescript@4.8.4/node_modules/valaxy/node_modules:/workspaces/takagisite/node_modules/.pnpm/valaxy@0.14.30_@types+markdown-it@12.2.3_@types+node@20.2.5_@unhead+vue@1.1.27_postcss@8.4.24_typescript@4.8.4/node_modules:/workspaces/takagisite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../valaxy/bin/valaxy.cjs" "$@"
else
  exec node  "$basedir/../valaxy/bin/valaxy.cjs" "$@"
fi
